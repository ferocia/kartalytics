    "@babel/preset-typescript": "^7.27.1",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/react": "^11.10.5",
    "@rails/actioncable": "^7.0.4",
    "@tanstack/react-query": "^4.14.1",
    "babel-loader": "^10.0.0",
    "chart.js": "^3.9.1",
    "core-js": "^3.26.0",
    "d3": "^7.6.1",
    "date-fns": "^2.29.3",
